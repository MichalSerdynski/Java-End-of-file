import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        boolean hasNext = true;
        int i = 1;
        String s = scan.nextLine();
        while(hasNext){
            System.out.println(i + " " + s);
            hasNext = scan.hasNext();
            if(hasNext){
                s = scan.nextLine();
                i++;
            }
        }
    }
}
